{
  "name": "swoleRefactor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "jest",
    "iPhone": "react-native run-ios --simulator='iPhone 6 Plus'",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon api/index.js",
    "db:start": "heroku pg:psql",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:undo": "sequelize db:migrate:undo --name"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "connect-ensure-login": "^0.1.1",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "helmet": "^3.9.0",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.4.0",
    "pg-promise": "^7.3.1",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-native": "^0.58.6",
    "react-native-calendars": "^1.22.0",
    "react-native-image-picker": "^0.28.1",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-pose": "^0.9.0",
    "react-native-svg": "^8.0.10",
    "react-native-webview": "^5.4.2",
    "react-redux": "^5.0.6",
    "react-router-native": "^4.1.1",
    "react-swipeable-views-native": "^0.13.2",
    "redux": "^3.7.2",
    "sequelize": "^4.43.1",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
